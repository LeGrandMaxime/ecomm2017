<?php

// src/ECommBundle/Entity/CommandeProduit.php


namespace ECommBundle\Entity;


use Doctrine\ORM\Mapping as ORM;


/**
* @ORM\Entity
* @ORM\Table(name="CommandeProduit")
*/
class CommandeProduit
{
    /**
    * @ORM\Column(name="id", type="integer")
    * @ORM\Id
    * @ORM\GeneratedValue(strategy="AUTO")
    */
    private $id;

    /**
    * @ORM\Column(name="nombre", type="integer")
    */
    private $nombre;

    /**
    * @ORM\ManyToOne(targetEntity="ECommBundle\Entity\Commande")
    * @ORM\JoinColumn(nullable=false)
    */

    private $Commande;
    /**
    * @ORM\ManyToOne(targetEntity="ECommBundle\Entity\Produit")
    * @ORM\JoinColumn(nullable=false)
    */

    private $Produit;

  


}
